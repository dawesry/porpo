{
  "name": "porpo",
  "author": "Ryan Dawes",
  "scripts": {
    "css-watch": "sass --watch --no-source-map _sass/:css/",
    "js-watch": "babel  --extensions .tsx _javascript --watch --out-dir lib/",
    "dev": "npm-run-all --parallel css-watch js-watch",
    "css-build": "sass --no-source-map _sass/:css/",
    "js-build": "babel --extensions .tsx _javascript --out-dir lib/",
    "build": "npm-run-all --parallel css-build js-build",
    "copy": "rm -rf dist && mkdir dist && cp CNAME *.html ./dist && cp -r css/ lib/ assets/ ./dist",
    "pages": "gh-pages -d ./dist",
    "clean": "rm -rf dist",
    "deploy": "npm-run-all --sequential build copy pages clean"
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/preset-react": "latest",
    "bulma": "latest",
    "gh-pages": "^4.0.0",
    "npm": "latest",
    "npm-run-all": "^4.1.5",
    "react-scripts": "latest",
    "sass": "latest",
    "typescript": "latest"
  }
}
